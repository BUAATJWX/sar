function [raw_scan, gps_data, metrics] = readFile(fileName)

% every new line is read in as a zero row
rawData = dlmread(fileName, ',', 1, 1);
dim = size(rawData);

data = zeros(dim)
% eliminate rows with all zeros 
for i = 0:ceil(dim(1)/2)
    data(i+1,:) = rawData(2*i + 1);
end 

data_size = size(data);

% separate radar data 
raw_scan = data(3:data_size(1),1:end-3);
gps_data = data(3:data_size(1),(end-3):end);
